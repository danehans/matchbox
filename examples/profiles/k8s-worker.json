{
	"id": "k8s-worker",
	"name": "Kubernetes Worker",
	"ignition_id": "k8s-worker.yaml",
	"boot": {
		"kernel": "/assets/coreos/1235.9.0/coreos_production_pxe.vmlinuz",
		"initrd": [
			"/assets/coreos/1235.9.0/coreos_production_pxe_image.cpio.gz"
		],
		"args": [
			"root=/dev/sda1",
			"coreos.config.url=http://gem-master01.ctocllab.cisco.com:8080/ignition?uuid=${uuid}\u0026mac=${mac:hexhyp}",
			"coreos.first_boot=yes",
			"console=tty0",
			"console=ttyS0",
			"coreos.autologin"
		]
	}
}
